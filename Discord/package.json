{
  "name": "notion-for-discord",
  "private": true,
  "version": "1.0.0",
  "description": "Notion for Discord",
  "main": "app.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
  "build": "npx tsc",
  "start": "node ./dist/app.js",
